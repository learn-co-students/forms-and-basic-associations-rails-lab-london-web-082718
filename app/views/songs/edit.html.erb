<h1>Editing <%= @song.title %></h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />
  <%= f.collection_select :genre_id, Genre.all.order(:name), :id, :name, prompt: true %>
  <% @song.note_contents.each do |note| %>
    <input name='song[note_contents][]' value="<%= note %>">
  <% end %>
  <%= f.submit %>
<% end %>
